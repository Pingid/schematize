{
  "name": "schematizr",
  "version": "3.0.3",
  "babel": {
    "presets": ["es2015", "stage-0"]
  },
  "description": "A functional library which converts deeply nested JSON to a schema which is easily editable.",
  "main": "lib/schema.js",
  "dependencies": {
    "ramda": "0.19.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.4.1",
    "mocha": "2.3.4"
  },
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "babel -d lib/ src/",
    "build-watch": "babel --watch -d lib/ src/",
    "test": "mocha --require babel-core/register test/index.js",
    "testing": "mocha --require babel-core/register test/index.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pingid/schematizr.git"
  },
  "author": "Dan Beaven <dm.beaven@gmail.com> (http://danbeaven.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pingid/schematizr/issues"
  },
  "homepage": "https://github.com/Pingid/schematizr#readme"
}
